<!-- Contact Us  -->
<section id="Contacts" class="contact-us" data-comp="Contact">
  <div class="container">
    <div class="row">
      <div class="contact-left">
        <!--<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3671.1436926786087!2d72.55631131454653!3d23.055192984936514!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x395e843fea762a7f%3A0x556fb8c750338958!2sGlorywebs!5e0!3m2!1sen!2sin!4v1491806480193" style="border:0" allowfullscreen=""  height="495"   ></iframe>-->
        {{!-- <div id="map"></div> --}}
      </div>
      <div class="contact-right">
        <h4 class="contact-title contact-title1"> <i class="fa fa-flag" aria-hidden="true"></i>{{title1}}</h4>
        <div class="contact-details">
          <p class="location-title">{{sub1}}</p>
          <p class="address"> <i class="fa fa-map-marker" aria-hidden="true"></i> 
              <span>{{address1}}<br>
              {{zip1}}</span> 
          </p>
          <hr>
          <h6 class="contact-title">{{title2}}</h6>
          <p class="contact-sub">{{sub2}}</p>
          <p class="address"> <i class="fa fa-map-marker" aria-hidden="true"></i>
            <span>{{address2}}<br>
              {{zip2}}</span>
          </p>
          <p class="contact-phone"> <i class="fa fa-phone" aria-hidden="true"></i> <a href="tel:+1{{phone}}"> +1
              {{phone}}</a></p>
          <p class="contact-email"> <i class="fa fa-envelope-o" aria-hidden="true"></i> <a
              href="mailto:{{email}}">{{email}}</a></p>
        </div>
        <a href="#" class="register-now-btn">{{button}}</a>
      </div>
    </div>
    {{!-- <p class="text-center"><a href="contact.html" class="register-now-btn styles-button">Contact Styles <i
          class="fa fa-long-arrow-right" aria-hidden="true"></i></a> </p> --}}

  </div>
</section>
<!-- /Contact Us -->